lambdaGateway.addRoutes({
path: '/{{lowerCase route}}/{id}',
methods: [ HttpMethod.PUT ],
integration: {{camelCase lambda}}DynamoIntegrations.putIntegration
});

lambdaGateway.addRoutes({
path: '/{{lowerCase route}}',
methods: [ HttpMethod.POST ],
integration: {{camelCase lambda}}DynamoIntegrations.postIntegration
});
lambdaGateway.addRoutes({
path: '/{{lowerCase route}}/{id}',
methods: [ HttpMethod.DELETE ],
integration: {{camelCase lambda}}DynamoIntegrations.delIntegration
});
lambdaGateway.addRoutes({
path: '/{{lowerCase route}}',
methods: [ HttpMethod.GET ],
integration: {{camelCase lambda}}DynamoIntegrations.getIntegration
});